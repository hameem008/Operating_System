+
+int main(int argc, char *argv[])
+{
+    if (argc < 3)
+    {
+        printf("Not enough argument\n");
+        exit(1);
+    }
+    int tickets = atoi(argv[1]);
+    int it = atoi(argv[2]);
+    settickets(tickets);
+    for (int i = 1; i <= 5; i++)
+    {
+        int pid = fork();
+        if (pid == 0)
+        {
+            run_dummy(1, it);
+            exit(1);
+        }
+    }
+    run_dummy(0, it);
+    return 0;
