+
+void run_dummy(int is_ch, int it)
+{
+    for (int i = 1; i <= it; i++)
+    {
+        if (i % 100000000 == 0)
+        {
+            sleep(1);
+        }
+        int dum_int = it / 2;
+        dum_int = (dum_int + dum_int - dum_int / 3) / (dum_int / 2);
+    }
